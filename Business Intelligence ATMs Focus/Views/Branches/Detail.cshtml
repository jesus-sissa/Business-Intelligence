<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Informacion Cajero Inteligente</h4>
                <!-- Nav tabs -->
                <ul class="nav nav-tabs" role="tablist">
                    <li class="nav-item"> <a class="nav-link active" data-toggle="tab" href="#equipo" role="tab"><span class="hidden-sm-up"><i class="ti-desktop"></i></span> <span class="hidden-xs-down">Cajero</span></a> </li>
                    <li class="nav-item"> <a class="nav-link" data-toggle="tab" href="#cash" role="tab"><span class="hidden-sm-up"><i class="ti-money"></i></span> <span class="hidden-xs-down">CashFlow</span></a> </li>
                    <li class="nav-item"> <a class="nav-link" data-toggle="tab" href="#valCas" role="tab"><span class="hidden-sm-up"><i class="ti-harddrives"></i></span> <span class="hidden-xs-down">Validadores y Casets</span></a> </li>

                </ul>
                <!-- Tab panes -->
                <div class="tab-content tabcontent-border">
                    <div class="tab-pane" id="cash" role="tabpanel">
                        <div class="p-20">
                            <div class="card">
                                <div class="card-title">
                                    <h4>Configuracion Cajero </h4>

                                </div>
                                <div class="card-body">
                                    <div class="table-responsive">
                                        <table class="table table-hover ">

                                            <tbody>
                                                 <tr>
                                                    <td>Plaza</td>
                                                    <td>
                                                        <span class="badge badge-success">@Model[0].Plaza</span
                                                    </td>

                                                </tr>
                                                 <tr>
                                                    <td>Remision Digital</td>
                                                    <td>
                                                        @if (Convert.ToBoolean(Model[0].RemisionDigital))
                                                        {
                                                            <span class="badge badge-success">On</span>
                                                        }
                                                        else
                                                        {
                                                            <span class="badge badge-warning">Off</span>
                                                        }

                                                    </td>

                                                </tr>
                                               
                                                
                                                <tr>
                                                    <td>Maneja Deposito Manual</td>
                                                    <td>
                                                        @if (Model[0].ManejaDepositoManual)
                                                        {
                                                            <span class="badge badge-success">On</span>
                                                        }
                                                        else
                                                        {
                                                            <span class="badge badge-warning">Off</span>
                                                        }

                                                    </td>

                                                </tr>
                                                <tr>
                                                    <td>Maneja Folio Manual </td>
                                                    <td>
                                                        @if (Model[0].ManejaFolioManual)
                                                        {
                                                            <span class="badge badge-success">On</span>
                                                        }
                                                        else
                                                        {
                                                            <span class="badge badge-warning">Off</span>
                                                        }

                                                    </td>

                                                </tr>
                                               <tr>
                                                    <td>Validar Folio </td>
                                                    <td>
                                                        @if (Model[0].VerificaFolio)
                                                        {
                                                            <span class="badge badge-success">On</span>
                                                        }
                                                        else
                                                        {
                                                            <span class="badge badge-warning">Off</span>
                                                        }

                                                    </td>

                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane active  p-20" id="equipo" role="tabpanel">
                        <div class="p-20">
                            <div class="card">
                                <div class="card-title">
                                    <h4>Informacion Equipo </h4>

                                </div>
                                <div class="card-body">
                                    <div class="table-responsive">
                                        <table class="table table-hover ">

                                            <tbody>
                                                <tr>
                                                    <td>Sistema Operativo</td>
                                                    <td>
                                                        @Model[0].Info[0].SO
                                                    </td>

                                                </tr>
                                                <tr>
                                                    <td>Disco</td>
                                                    <td>
                                                        @Model[0].Info[0].DD
                                                    </td>

                                                </tr>
                                                <tr>
                                                    <td>Memoria Ram</td>
                                                    <td>
                                                        @Model[0].Info[0].RAM

                                                    </td>

                                                </tr>
                                                <tr>
                                                    <td>Procesador</td>
                                                    <td>
                                                        @Model[0].Info[0].Procesador
                                                          <span class="badge badge-primary">@Model[0].Info[0].ProcesadorStatus</span>
                                                    </td>

                                                </tr>
                                                 <tr>
                                                    <td>Procesador Nucleos</td>
                                                    <td>
                                                        @Model[0].Info[0].Procesador_Nucleos
                                                    </td>

                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane" id="valCas" role="tabpanel">
                        <div class="row">
                            <div class="col-lg-6 col-xl-6 col-xxl-6">
                                <div class="card">
                                    <div class="card-title">
                                        <h4>Validadores </h4>

                                    </div>
                                    @foreach (var item in Model[0].Validadores)
                                    {

                                        <div class="col-lg-12">
                                            <div class="card p-0">
                                                <div class="stat-widget-three home-widget-three">
                                                    <div class="stat-icon bg-facebook">
                                                        <i class="ti-harddrives"></i>
                                                    </div>
                                                    <div class="stat-content">
                                                        <div class="stat-digit">Validador</div>
                                                        @if (@item.Activo)
                                                        {
                                                            <span class="badge badge-success">Activado</span>

                                                        }
                                                        else
                                                        {
                                                            <span class="badge badge-warning">Desactivado</span>
                                                        }
                                                    </div>
                                                    @if (@item.Activo)
                                                    {
                                                        <div class="progress">
                                                            <div class="progress-bar progress-bar-success" style="width:100%" role="progressbar" aria-valuenow="85" aria-valuemin="0" aria-valuemax="100"></div>
                                                        </div>
                                                    }
                                                    else
                                                    {
                                                        <div class="progress">
                                                            <div class="progress-bar progress-bar-warning " style="width:100%" role="progressbar" aria-valuenow="85" aria-valuemin="0" aria-valuemax="100"></div>
                                                        </div>
                                                    }

                                                </div>
                                            </div>
                                        </div>
                                    }
                                </div>
                            </div>
                            <div class="col-lg-6 col-xl-6 col-xxl-6">
                                <div class="card">
                                    <div class="card-title">
                                        <h4>Cartuchos </h4>

                                    </div>
                                    @foreach (var item in Model[0].Casets)
                                    {
                                        <div class="col-lg-12">
                                            <div class="card p-0">
                                                <div class="stat-widget-three home-widget-three">
                                                    <div class="stat-icon bg-facebook">
                                                        <i class="ti-wallet"></i>
                                                    </div>
                                                    <div class="stat-content">
                                                        <div class="stat-text">Limite @item.Capacidad pzs</div>
                                                        <div class="stat-digit">@item.Cantidad_Actual pzs</div>
                                                    </div>
                                                    <div class="progress">
                                                        <div class="progress-bar progress-bar-success " style="width:@(item.Cantidad_Actual/item.Capacidad*100)%!important" id="pgr_@caset" role="progressbar" aria-valuenow="85" aria-valuemin="0" aria-valuemax="100"></div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>


                                        caset++;
                                    }

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>




